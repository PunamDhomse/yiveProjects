import React from 'react';
import { Link } from 'react-router-dom';
import { connect } from 'react-redux';
import Image from 'react-bootstrap/Image';
import { userActions, sidebarActions } from '../_actions';

class Sidebar extends React.Component {
  constructor(props) {
    super(props);
    this.state = {
      active: '',
      wrapperRef: null
    };

    this.toggleClass = this.toggleClass.bind(this);
  }

  componentWillMount() {
    document.addEventListener('click', this.handleClickOutside);
  }

  componentWillUnmount() {
    document.removeEventListener('click', this.handleClickOutside);
  }

  handleClickOutside = (event) => {
    var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
    if (width <= 768) {
      if (this.state.wrapperRef && !this.state.wrapperRef.contains(event.target)) {
        this.props.toggle(false);
      }
    }
  }

  toggleClass() {
    if (this.state.active == '') {
      this.setState({ active: 'open' });
    } else {
      this.setState({ active: '' });
    }
    this.props.toggle(!this.props.sidebar);
  };

  setRef = (node) => {
    this.setState({ wrapperRef: node })
  }

  render() {
    const { user } = this.props;
    return (
      <aside ref={this.setRef} className={this.props.sidebar ? "side-nav open" : 'side-nav'} id="show-side-navigation1">
        <div className="heading">
          <div className="info">
            <div className="logo text-center">
              <a href="/dashboard"><img src={this.props.sidebar ? `data:image/png;base64,` : `data:image/png;base64,`} /></a>
            </div>
            <i class="fa fa-arrow-left fa-fw closeNav" onClick={this.toggleClass} ></i>
          </div>
        </div>
        <ul className="categories">
          <li className={['/dashboard'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/dashboard"><i className="fa fa-home fa-fw" aria-hidden="true"></i> <span>Dashboard</span></Link></li>

          <li className={['/customer', '/addcustomer', '/editcustomer/:customer_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/customer"><i className="fa fa-users fa-fw"></i> <span>Individuals</span></Link></li>

          {user && ['admin', 'businessOwner'].includes(user.role) ? <li className={['/business', '/addbusiness', '/editbusiness/:business_id', '/locationMgr/:b_id', '/addlocationMgr/:b_id', '/editlocationMgr/:locationMgr_id/:b_id', '/business-manager/:b_id', '/addbusinessmanager/:b_id', '/editbusinessmanager/:mgr_id/:b_id', '/create-individual/:b_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/business"><i className="fa fa-building fa-fw"></i>  <span>Businesses</span></Link>
		 
		  </li> : null}

          {/* {user && ['admin', 'businessOwner'].includes(user.role) ? <li className={['/business-manager', '/addbusinessmanager', '/editbusinessmanager/:mgr_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/business-manager"><i className="fa fa-user fa-fw"></i> <span>Business Manager</span></Link></li> : null} */}


          {/* {user && ['admin', 'businessOwner'].includes(user.role) ?
            <li className={['/locationMgr', '/addlocationMgr', '/editlocationMgr/:locationMgr_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/locationMgr"><i className="fa fa-user fa-fw"></i> <span>Location Manager</span></Link></li>
            : ''} */}

          {/* <li><Link to="/visitor"><i className="fa fa-users fa-fw"></i> <span>Visitors</span></Link></li> */}
		  
		  <li className={['/devices', 'adddevice', '/editdevice/:device_id'].includes(this.props.match.path) ? ' submenu active_side' : 'submenu'}><Link to="/devices"><i className="fa fa-mobile-phone fa-fw"></i> <span>Devices</span></Link></li>
		  

          <li className={['/reports', '/visitor/:loc_id/:bus_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/reports"><i className="fa fa-bar-chart fa-fw"></i> <span>Reports</span></Link></li>

          {/*user && user.role == 'admin' ?
            <li className={['/users', '/adduser', '/edituser/:user_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/users"><i className="fa fa-user fa-fw"></i> <span>Admin Users</span></Link></li>
            : ''*/}

          {/* {user && ['admin', 'businessOwner'].includes(user.role) ?
            <li className={['/devices', '/adddevice', '/editdevice/:device_id'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/devices"><i className="fa fa-building"></i> <span>Devices</span></Link></li>
            : ''} */}

          <li className={['/settings'].includes(this.props.match.path) ? 'active_side' : ''}><Link to="/settings"><i className="fa fa-cog fa-fw"></i> <span>Settings</span></Link></li>
          {/* <li><Link to="/changepassword"><i className="fa fa-key fa-fw"></i> <span>Change Password</span></Link></li> */}
        </ul>
      </aside>
    );
  }
}

function mapState(state) {
  const { user } = state.authentication;
  const { sidebar, common } = state;
  return { user, sidebar, common };
}

const actionCreators = {
  toggle: sidebarActions.toggle
}

const connectedSidebar = connect(mapState, actionCreators)(Sidebar);
export { connectedSidebar as Sidebar };