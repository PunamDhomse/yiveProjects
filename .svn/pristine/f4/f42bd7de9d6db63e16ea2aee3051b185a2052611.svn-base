/**** custome sidebar css ****/
.side-nav .categories > li{padding:0 !important;}
.side-nav .categories > li a{padding:17px 24px; display:block;}
.dropdown-toggle:after{display:none;}
.custom-dropdown{    position: static !important;transform: none !important;width: 100%;background: #000;border-radius: 0;padding: 0;}
.custom-dropdown li{padding:0 0px 0 30px !important;}
.custom-dropdown li a{color:#fff !important;padding:15px;}

/* .side-nav .categories > li.submenu {padding:0 0 0 30px!important; }
.submenu a{padding:10px !important;}
.side-nav .categories li.submenu.active_side{background: #6bbe49 !important;}
.submenu i.fa{font-size:26px !important;float: left;} */

.custom-tab-setting{float:none !important;}
.custom-tab-setting a{position:static !important;}

/**** sidebar width change ****/
.side-nav{width:165px;}
.wrapper, .header .navbar{width: calc(100% - 165px);}
.side-nav .logo img{width:44%;}

@media (min-width:992px){
.side-nav.open .categories > li.submenu{padding: 0 0 0 !important;background: inherit !important;}
.side-nav.open .categories > li.submenu > a{padding: 17px 24px !important;}
.side-nav.open .submenu i.fa{float:none; margin:0 0 0 -7px;font-size:32px !important;}
.side-nav.open .categories li.submenu.active_side{background: #6bbe49 !important;}

}
@media (max-width:991px){
/**** sidebar width change ****/
.side-nav{width:200px;left: -200px;}
.side-nav.open{width:200px;}
.wrapper, .header .navbar{width: 100%;}
.side-nav.open .logo img{width:51%;}
}
.alert-danger{
  margin-top: 50px;
}