.logo img {
  width: 50%;
}
.wrapper {
    /*width: calc(100% - 80px);*/
    width: calc(100% - 250px);
    float: right;
    height: 100vh;
    background: white;
    overflow-y: scroll;
}
.row > .container-fluid {
    margin-top: 45px;
}
/*Header*/
.header .navbar {
    background: #343a40;
    padding: 10px 15px;
    float: right;
    width: calc(100% - 250px);
    left: inherit;
}
.header .navbar a {
    color: #fff !important;
}
.header .dropdown-menu {
  right: 0;
  left: initial;
}
.header .navbar .dropdown a.dropdown-item {
    color: #3a4551 !important;
    padding: 6px 15px;
}
.header .navbar .dropdown a.dropdown-item:hover, .header .navbar .dropdown a.dropdown-item:active, .header .navbar .dropdown a.dropdown-item:focus {
    background: #454f5d !important;
    color: #fff !important;
    outline: none;
    box-shadow: none !important;
}
.header .dropdown-menu.show, .header .dropdown-divider {
    padding: 0;
    margin: 0;
}
.header .navbar .dropdown a.dropdown-item .fa {
    font-size: 12px;
    margin-right: 6px;
}
.dropdown-toggle {
  display: flex;
  flex-wrap:wrap;
  align-items: center;
}
.header .dropdown-toggle::after {
    margin-top: 6px !important;
}
.header .navbar .dropdown a.dropdown-item:hover .fa {
    color: #fff;
}

/* Start side navigation bar  */
.side-nav {
  float: left;
  height: 100%;
  /*width: 80px;*/
  width: 250px;
  background-color:black;
  color: #fff;
  -webkit-transform: translateX(0);
  -moz-transform: translateX(0);
  transform: translateX(0);
  -webkit-transition: all .3s ease-in-out;
  -moz-transition: all .3s ease-in-out;
  transition: .3s;
  position: fixed;
  top: 0;
  left: 0;
  /*overflow: auto;*/
  overflow: initial;
  z-index: 999;
}
.side-nav .categories > li > a {
    color: #fff !important;
    text-decoration: none;
    /*display: none;*/
}
.side-nav.open .categories > li > a span {
    display: none;
}
.side-nav.open ~ .wrapper > .header > .navbar {
    width: calc(100% - 80px);
}
.yive {
	display: none;
}
.closeNav {
    position: absolute;
    top: 8px;
    right: 8px;
    font-size: 14px;
    cursor: pointer;
}
/* .side-nav:hover .categories > li > a, .side-nav.open .yive {
    display: initial;
} */
.side-nav .logo .l {
	background-color: #fff; 
}
.side-nav .close-aside {
  position: absolute;
  top: 7px;
  right: 7px;
  cursor: pointer;
  color: #EEE;
}
.side-nav .logo {
    /*padding: 20.5px 0;*/
    padding: 9px 0;
}
.side-nav.open ~ .wrapper {
    width: calc(100% - 80px);
}
.side-nav.open .logo {
    padding: 14px 3px;
}
.side-nav.open .closeNav:before {
    content: "\f061";
}
.side-nav.open .closeNav {
    right: 3px;
    top: 4px;
    font-size: 14px;
}
.side-nav .logo img {
    width: 35%;
}
.side-nav.open .logo img {
    width: 40%;
    max-height: 2em;
}
.side-nav .heading {
  	background-color: black;
    padding: 0;
    overflow: hidden;
    border-bottom: 1px solid #ffffff;
}
.side-nav .heading > img {
  border-radius: 50%;
  float: left;
  width: 28%;
}
.side-nav .info {
  float: left;
  width: 100%;
  margin-left: 0;
}
.side-nav .heading .info > h3 {margin: 0 0 5px}
.side-nav .heading .info > h3 > a {
  color: #fff !important;
  font-weight: 100;
  margin-top: 4px;
  display: block;
  text-decoration: none;
  font-size: 18px;
}
.side-nav .heading .info > h3 > a:hover {
  color: #FFF;
}
.side-nav .heading .info > p {
  color: #fff;
  font-size: 13px;
}
/* End heading */
/* Start search */
.side-nav .search {
  text-align: center;
  padding: 15px 30px;
  margin: 15px 0;
  position: relative;
}
.side-nav .search > input {
  width: 100%;
  background-color: transparent;
  border: none;
  border-bottom: 1px solid #23262d;
  padding: 7px 0 7px;
  color: #DDD
}
.side-nav .search > input ~ i {
  position: absolute;
  top: 22px;
  right: 40px;
  display: block;
  color: #2b2f3a;
  font-size: 19px;
}
/* End search */
.side-nav .categories > li:hover {
    background: #2d363f;
}
.side-nav .categories > li > a {
  color: #fff !important;
  text-decoration: none;
}
.side-nav .categories > li {
  padding: 17px 24px;
  overflow: hidden;
  border-bottom: 1px solid rgba(255, 255, 255, 0.22);
  cursor: pointer;
}
.side-nav.open .categories > li {
	padding: 15px 25px;
}
.side-nav .categories > li > i {
    font-size: 18px;
    margin-right: 5px;
}
.side-nav.open .categories > li i {
    font-size: 24px;
    margin-right: 0px;
}
/* Start num: there are three options primary, danger and success like Bootstrap */
.side-nav .categories > li > a > .num {
  line-height: 0;
  border-radius: 3px;
  font-size: 14px;
  color: #FFF;
  padding: 0px 5px
}
.dang {background-color: #f35959}
.prim {background-color: #0275d8}
.succ {background-color: #5cb85c}
/* End num */
.side-nav .categories > li > a:hover {
  color: #FFF
}
.side-nav .categories > li:last-child > a:after {
  /*content: "\f053";*/
  font-family: fontAwesome;
  font-size: 11px;
  line-height: 1.8;
  float: right;
  color: hsla(0,0%,100%,.5);
  -webkit-transition: all .3s ease-in-out;
  -moz-transition: all .3s ease-in-out;
  transition: all .3s ease-in-out;
}
.side-nav .categories .opend > a:after {
  -webkit-transform: rotate(-90deg);
  -moz-transform: rotate(-90deg);
  transform: rotate(-90deg);
}
/* End categories */
/* Start dropdown menu */
.side-nav .categories .side-nav-dropdown {
  padding-top: 10px;
  padding-left: 30px;
  list-style: none;
  display: none;
}
.side-nav .categories .side-nav-dropdown > li > a {
  color: #fff !important;
  text-decoration: none;
  padding: 7px 0;
  display: block;
}
.side-nav .categories p {
  margin-left: 30px;
  color: #535465;
  margin-top: 10px;
}
.side-nav.open {
    width: 80px;
}
/*for active link*/
.side-nav .categories .active_side {
    background: #6bbe49;
}

/* Start bootstrap */
.navbar-right .dropdown-menu {
  right: auto !important;
  left: 0 !important;
}
.navbar-default {
  	border: none !important;
    border-radius: 0 !important;
    margin: 0 !important;
    background: #45515d;
}
.navbar-default .navbar-nav>li>a {
  color: #EEE !important;
  line-height: 55px !important;
  padding: 0 10px !important;
}
.navbar-default .navbar-brand {color:#FFF !important}
.navbar-default .navbar-nav>li>a:focus,
.navbar-default .navbar-nav>li>a:hover {color: #EEE !important}

.navbar-default .navbar-nav>.open>a,
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover {background-color: transparent !important; color: #FFF !important}

.navbar-brand .navbar-brand {
  line-height: 55px !important; 
  padding: 0 !important
}
.navbar-default .navbar-brand:focus,
.navbar-default .navbar-brand:hover {color: #FFF !important}
.navbar>.container .navbar-brand, .navbar>.container-fluid .navbar-brand {margin: 0 !important}

